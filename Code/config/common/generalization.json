{
    "name": "Eval-genetation",
    "n_gpu": 1,

    "data_loader": {
        "type": "TCR_Antigen_Dataset_AbDab",
        "args":{
            "seed": 2,
            "batch_size": 16,
            "validation_split": 0,
            "test_split": 0,
            "num_workers": 8,
            "data_dir": "../General.csv",
            "tcr_vocab_dir": "../ProcessedData/vocab/beta-2-3.csv",
            
            "tcr_tokenizer_dir": "/data/coding/TCRBinder-main/Result_beta/checkpoints/Pretrain/XXXX-XXXXXX",
            
            "tokenizer_name": "common",
            
            "token_length_list": "2,3",
            
"antigen_seq_name": "Antigen",
      "beta_seq_name": "TCR_Beta",
      "alpha_seq_name": "TCR_Alpha",
      "hla_seq_name": "HLA",
            "label_name":"frequence",
            "beta_max_len": 130,
            "alpha_max_len": 130,
            "antigen_max_len": 13,
	        "hla_max_len": 34,
            "test_tcrs": 1,
            "shuffle": false
        }
    },

    "arch": {
        "type": "BERTBinding_AbDab_cnn",
        "args": {
            "beta_dir": "/data/coding/TCRBinder-main/Result_beta/checkpoints/Pretrain/XXXX-XXXXXX",
            "alpha_dir": "/data/coding/TCRBinder-main/Result_alpha/checkpoints/Pretrain/XXXX-XXXXXX",
            "antigen_dir": "/data/coding/TCRBinder-main/Code/esm2/esm2_150m/",
	    "hla_dir": "/data/coding/TCRBinder-main/Code/esm2/esm2_150m/",
            "emb_dim": 256
        }
    },
    "discriminator_resume": "/data/coding/TCRBinder-main/Result_PHT/checkpoints/Finetunning-TCR-Binding/XXXX-XXXXXX/model_best.pth",


    "optimizer": {
        "type": "AdamW",
        "args":{
            "lr": 8e-6,
            "weight_decay": 0
        }
    },
    "loss": "mse_loss",
    "is_regression": true,
    "metrics": [
        "mse", "get_spearman", "get_pearson"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 10,
            "gamma": 0.5
        }
    },
    "trainer": {
        "epochs": 1,
        
        "save_dir": "../TCR_general/",
        "save_period": 50,
        "verbosity": 2,
        "logging_steps":25,
        "monitor": "min val_loss",
        "early_stop": 0,

        "tensorboard": false
    }
}
